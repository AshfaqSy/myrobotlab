
  public synchronized void %name%(%javaMethodParameters%) {
    log.info("Sending Messge: %name%");
    try {
      startMessage();
      appendMessage(MAGIC_NUMBER);
      appendMessage(%javaWriteMsgSize%); // size
%javaWrite% 
      sendMessage();
      if (ackEnabled){
        waitForAck();
      }
%javaSendRecord%
	} catch (Exception e) {
      log.error("%name% threw",e);
    }
  }
